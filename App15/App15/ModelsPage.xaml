                         Clicked="ButtonCclass_Clicked"/>

                <ImageButton Source="cla.jpg"
                         Grid.Row="1" Grid.Column="0"
                         BackgroundColor="White"
                         x:Name="buttonCla"
                         Clicked="ButtonCla_Clicked"/>
                <ImageButton Source="cls.jpg"
                         Grid.Row="1" Grid.Column="1"
                         BackgroundColor="White"
                         x:Name="buttonCls"
                         Clicked="ButtonCls_Clicked"/>
                <ImageButton Source="eclass.jpg"
                         Grid.Row="1" Grid.Column="2"
                         BackgroundColor="White"
                         x:Name="buttonEclass"
                         Clicked="ButtonEclass_Clicked"/>

                <ImageButton Source="eqc.jpg"
                         Grid.Row="2" Grid.Column="0"
                         BackgroundColor="White"
                         x:Name="buttonEqc"
                         Clicked="ButtonEqc_Clicked"/>
                <ImageButton Source="gclass.jpg"
                         Grid.Row="2" Grid.Column="1"
                         BackgroundColor="White"
                         x:Name="buttonGclass"
                         Clicked="ButtonGclass_Clicked"/>
                <ImageButton Source="gla.jpg"
                         Grid.Row="2" Grid.Column="2"
                         BackgroundColor="White"
                         x:Name="buttonGla"
                         Clicked="ButtonGla_Clicked"/>

                <ImageButton Source="glb.jpg"
                         Grid.Row="3" Grid.Column="0"
                         BackgroundColor="White"
                         x:Name="buttonGlb"
                         Clicked="ButtonGlb_Clicked"/>
                <ImageButton Source="glc.jpg"
                         Grid.Row="3" Grid.Column="1"
                         BackgroundColor="White"
                         x:Name="buttonGlc"
                         Clicked="ButtonGlc_Clicked"/>
                <ImageButton Source="gle.jpg"
                         Grid.Row="3" Grid.Column="2"
                         BackgroundColor="White"
