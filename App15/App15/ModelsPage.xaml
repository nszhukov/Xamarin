<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App15.ModelsPage"
             Title="Vehicles">
    <ContentPage.Content>
        <StackLayout>
            <Grid BackgroundColor="White">

                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>

                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>

                <ImageButton Source="aclass.jpg"
                         Grid.Row="0" Grid.Column="0"
                         BackgroundColor="White"
                         x:Name="buttonAclass"
                         Clicked="ButtonAclass_Clicked"/>
                <ImageButton Source="bclass.jpg"
                         Grid.Row="0" Grid.Column="1"
                         BackgroundColor="White"
                         x:Name="buttonBclass"
                         Clicked="ButtonBclass_Clicked"/>
                <ImageButton Source="cclass.jpg"
                         Grid.Row="0" Grid.Column="2"
