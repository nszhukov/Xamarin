                         Grid.Row="4" Grid.Column="2"
                         BackgroundColor="White"
                         x:Name="buttonSl"
                         Clicked="ButtonSl_Clicked"/>

                <ImageButton Source="slc.jpg"
                         Grid.Row="5" Grid.Column="0"
                         BackgroundColor="White"
                         x:Name="buttonSlc"
                         Clicked="ButtonSlc_Clicked"/>
                <ImageButton Source="vclass.jpg"
                         Grid.Row="5" Grid.Column="1"
                         BackgroundColor="White"
