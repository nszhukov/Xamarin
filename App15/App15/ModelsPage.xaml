                         Clicked="ButtonVclass_Clicked"/>
                <ImageButton Source="eqv.jpg"
                         Grid.Row="5" Grid.Column="2"
                         BackgroundColor="White"
                         x:Name="buttonEqv"
                         Clicked="ButtonEqv_Clicked"/>

                <ImageButton Source="xclass.jpg"
                         Grid.Row="6" Grid.Column="0"
                         BackgroundColor="White"
                         x:Name="buttonXclass"
                         Clicked="ButtonXclass_Clicked"/>
                <ImageButton Source="amggt.jpg"
                         Grid.Row="6" Grid.Column="1"
                         BackgroundColor="White"
